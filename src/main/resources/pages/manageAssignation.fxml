<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="600.0" styleClass="bg-success">
         <children>
            <Label fx:id="title" styleClass="text-light" text="Asignar">
               <font>
                  <Font name="Yu Gothic UI Bold" size="36.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="20.0" top="20.0" />
         </padding>
      </HBox>
      <VBox alignment="CENTER" maxWidth="800.0" prefHeight="115.0" prefWidth="600.0">
         <children>
            <VBox maxWidth="300.0" prefHeight="81.0" prefWidth="560.0" spacing="30.0">
               <children>
                  <VBox prefHeight="100.0" prefWidth="100.0">
                     <children>
                        <Label text="Alumno" />
                        <ComboBox fx:id="student" maxWidth="Infinity" prefWidth="150.0" styleClass="combo-box" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <VBox prefHeight="100.0" prefWidth="100.0">
                     <children>
                        <Label text="Tutor" />
                        <ComboBox fx:id="tutor" maxWidth="Infinity" prefWidth="317.0" styleClass="combo-box" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
                  <VBox prefHeight="100.0" prefWidth="100.0">
                     <children>
                        <Label text="Empresa" />
                        <ComboBox fx:id="company" maxWidth="Infinity" prefWidth="317.0" styleClass="combo-box" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <styleClass>
                  <String fx:value="alert-container" />
                  <String fx:value="alert-success" />
               </styleClass>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="30.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="600.0">
         <children>
            <Button fx:id="submit" mnemonicParsing="false" onAction="#submitForm" text="Asignar">
               <font>
                  <Font size="15.0" />
               </font>
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-success" />
               </styleClass>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" top="20.0" />
         </padding></HBox>
   </children>
</VBox>
